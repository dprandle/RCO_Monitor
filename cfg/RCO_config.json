{
    "logging_enabled": true,
    "simulate_radios": false,
    "ip_lower_bound": 10,
    "ip_upper_bound": 119,
    "max_radio_retry_reconnect_count": -1,
    "connection_timeout": {
        "seconds": 0,
        "microseconds": 100000
    },
    "loggers": {
        "PTT_SQ_Active_Changed_250ms": {
            "dir_path":"/home/ubuntu/csvlogs",
            "period": 200,
            "ptt_status": {
                "title": "PTT",
                "change": true,
                "equal":"LOCAL | REMOTE | TEST_RF"
            },
            "squelch_status": {
                "title": "Squelch",
                "change":true,
                "equal":"OPEN"
            },
            "forward_power": {
                "title": "Fwd Pwr"
            },
            "vswr": {
                "title": "VSWR"
            },
            "agc": {
                "title": "AGC"
            },
            "line_level": {
                "title": "Line Lvl"
            }
        },
        "High_VSWR_500ms": {
            "dir_path":"/home/ubuntu/csvlogs",
            "period": 1000,
            "forward_power": {
                "title": "Fwd Pwr"
            },
            "vswr": {
                "title":"VSWR",
                "greater_than": 1.7
            }
        }
    }
}